extend type Query {
  japaneseWords(query: String!): JapaneseWords
}

type JapaneseWords {
  words: [JapaneseWord!]
}

type JapaneseWord {
  kanji: String!
  furigana: String
  hiragana: String!
  acents: String!
  meaning: String!
  # Links to audio files
  audio: [String!]
  examples: [String!]
}
